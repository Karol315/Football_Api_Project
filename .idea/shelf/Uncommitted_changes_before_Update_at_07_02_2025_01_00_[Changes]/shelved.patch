+/* ⚽ Główne tło - wygląd murawy piłkarskiej */
+.root {
+    -fx-background-color: linear-gradient(to bottom, #2b8a3e, #1e5631);
+    -fx-font-family: "Arial";
+}
+
+/* 🏷️ Styl etykiet (np. nazwa drużyny) */
+.label {
+    -fx-font-size: 16px;
+    -fx-text-fill: white;
+    -fx-font-weight: bold;
+}
+
+.button {
+    -fx-background-color: #4CAF50;
+    -fx-background-radius: 12px;
+    -fx-text-fill: white;
+    -fx-font-size: 14px;
+    -fx-padding: 10px 20px;
+    -fx-border-radius: 12px;
+    -fx-border-color: #388E3C;
+    -fx-border-width: 2px;
+    -fx-transition: all 0.3s ease;
+}
+
+.button:hover {
+    -fx-background-color: #388E3C;
+    -fx-background-radius: 12px;
+    -fx-border-color: #2C6A2F;
+    -fx-border-width: 3px;
+    -fx-scale-x: 1.1;
+    -fx-scale-y: 1.1;
+}
+
+.button:pressed {
+    -fx-background-color: #2C6A2F;
+    -fx-border-color: #1B4D26;
+    -fx-scale-x: 0.95;
+    -fx-scale-y: 0.95;
+}
+
+.button > .label {
+    -fx-font-weight: bold;
+    -fx-font-size: 16px;
+}
+
+.button.background-football {
+    -fx-background-image: url('/images/football-icon.png');
+    -fx-background-size: 30px 30px;
+    -fx-background-position: left center;
+    -fx-padding: 10px 20px 10px 40px;
+}
+
+/* 📜 Styl pola wyszukiwania */
+.text-field {
+    -fx-background-color: white;
+    -fx-text-fill: black;
+    -fx-border-color: #0b3d91;
+    -fx-border-radius: 5px;
+}
+
+/* 🎨 Styl ComboBoxa (sortowania graczy) */
+.combo-box {
+    -fx-background-color: white;
+    -fx-border-color: #0b3d91;
+    -fx-border-radius: 5px;
+    -fx-text-fill: black;
+}
+
+/* 🏟️ Tło dla okna drużyny */
+#team-container {
+    -fx-background-color: rgba(0, 0, 0, 0.5);
+    -fx-padding: 10px;
+    -fx-border-radius: 10px;
+}
+
+/* 🏆 Styl informacji o zawodnikach */
+.player-box {
+    -fx-background-color: #f4f4f4;
+    -fx-border-radius: 10px;
+    -fx-padding: 10px;
+    -fx-alignment: center;
+}
+
+.player-box:hover {
+    -fx-background-color: #ddd;
+}
